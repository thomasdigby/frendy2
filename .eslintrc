{
  "rules": {
    "comma-dangle": ["error", "never"],
    "semi": ["error", "never"],
    "import/no-unresolved": [2, {
      "ignore": [".css"]
    }]
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  }
}
