{
  "rules": {
    "comma-dangle": ["error", "never"],
    "semi": ["error", "never"],
    "consistent-return": 0,
    "no-underscore-dangle": 0,
    "padded-blocks": 0,
    "no-use-before-define": 0,
    "no-param-reassign": 0
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  }
}
